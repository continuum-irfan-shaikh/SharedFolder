CREATE TABLE IF NOT EXISTS script_execution_results (
    managed_endpoint_id uuid,
    task_instance_id    uuid,
    execution_status    int,
    exit_code           int,
    std_err             text,
    std_out             text,
    updated_at          timestamp,
    PRIMARY KEY         (managed_endpoint_id, task_instance_id)
) WITH CLUSTERING ORDER BY (task_instance_id DESC);
