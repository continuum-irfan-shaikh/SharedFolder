{ 
    "name": "patchingResponseMessage",
    "type":  "resource",
    "version": "1.0",
    "attributes": [
        { "name": "executionID",  "type": "string", "description": "ExecutionID generated by Scripting MicroService"},
        { "name": "status",       "type": "string", "description": "Return status Success/Failed"},
